{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run lint && npm run build-ts",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"{src,types}/**/*.{js,ts}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\" \"types/**/*.ts\"",
    "serve-debug": "nodemon --inspect dist/src/server.js",
    "serve": "node dist/src/server.js",
    "start": "npm run serve",
    "test": "NODE_ENV=test jest --forceExit --runInBand --cache --verbose --detectOpenHandles ./tests",
    "test:watch": "NODE_ENV=test jest --forceExit --verbose --detectOpenHandles --watchAll --runInBand --cache ./tests",
    "test:coverage": "NODE_ENV=test jest --forceExit --coverage --verbose --detectOpenHandles ./tests",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon src/server.ts",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.15",
    "@types/errorhandler": "^1.5.3",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "axios-mock-adapter": "^1.22.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
